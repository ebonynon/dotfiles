{
	"name": "vscode-rustfmt",
	"displayName": "Rustfmt",
	"description": "document formatter for single rust file",
	"publisher": "statiolake",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other",
		"Programming Languages",
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:rust"
	],
